<p id="notice"><%= notice %></p>

<h1>Qualities</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @qualities.each do |quality| %>
      <tr>
        <td><%= quality.name %></td>
        <td><%= link_to 'Show', quality %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>